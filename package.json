{
  "name": "dota-patch-bot",
  "version": "1.0.0",
  "description": "Discord bot for Dota 2 patch notifications",
  "main": "index.js",
  "type": "module",
  "repository": {
    "url": "https://github.com/pendo324/dota-patch-bot"
  },
  "scripts": {
    "build": "tsc",
    "dev-server": "tsc-watch -p ./tsconfig.json --noClear --onSuccess \"node ./dist/index.mjs\"",
    "register-commands": "node dist/register-commands.mjs",
    "start": "node dist/index.mjs",
    "watch": "tsc -w"
  },
  "exports": {
    "./*": "./dist/src/*"
  },
  "keywords": [
    "dota2"
  ],
  "author": "pendo324",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^0.7.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "axios": "^0.24.0",
    "discord-api-types": "^0.25.0",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "node-schedule": "^2.1.0"
  },
  "devDependencies": {
    "@types/node-schedule": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.0",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.6.0-dev.20211130"
  },
  "resolutions": {
    "ow": "0.27.0"
  }
}
